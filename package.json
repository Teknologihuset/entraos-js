{
  "name": "entraos.js",
  "version": "0.0.1",
  "description": "JavaScript client to communicate EntraOS rest endpoints",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint",
    "lint-fix": "eslint --fix",
    "prepare": "husky install",
    "start": "node dist/entraos.js",
    "watch": "concurrently \"npm run build --watch\" \"nodemon -q dist/entraos.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Teknologihuset/entraos-js.git"
  },
