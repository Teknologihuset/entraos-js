    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.7"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fp-ts": "^2.12.2",
    "moment": "^2.29.4",
    "openid-client": "^5.1.8",
    "ts-pattern": "^4.0.5"
